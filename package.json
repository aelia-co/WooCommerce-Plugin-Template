{
  "name": "woocommerce-template-plugin",
  "description": "A template for WooCommerce Plugins",
  "version": "0.1.0",
  "license": "GPL-3.0",
  "devDependencies": {
    "grunt": ">=0.4.5",
    "grunt-rename": ">=0.1.4",
    "grunt-string-replace": ">=1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aelia-co/woocommerce-template-plugin.git"
  },
  "homepage": "https://aelia.co",
  "author": {
    "name": "Aelia",
    "email": "support@aelia.co",
    "url": "https://aelia.co"
  },
  "bugs": "https://github.com/aelia-co/woocommerce-template-plugin/issues",
  "plugin_info": {
    "PLUGIN_NAME": "WooCommerce Template Plugin",
    "PLUGIN_NSPACE": "Aelia\\WC\\WC_Template_Plugin",
    "PLUGIN_CLASS_NAME": "WC_Template_Plugin",
    "PLUGIN_SLUG_VALUE": "woocommerce-template-plugin",
    "PLUGIN_MENU_SLUG": "wc_template_plugin",
    "PLUGIN_TEXT_DOMAIN": "woocommerce-template-plugin",
    "PLUGIN_SUPPORT_URL": "https://github.com/aelia-co/woocommerce-template-plugin/issues",
    "PLUGIN_CONTACT_URL": "https://github.com/aelia-co/woocommerce-template-plugin/issues",
		// REMOVE THIS COMMENT BEFORE RUNNING THE BUILD SCRIPT
		// Make sure that the settings key is different from the plugin slug, as they serve different
		// purposes.
		// - The plugin slug will be used to create an option (function update_option()) with the installed version of the plugin.
		// - The settings key will be used to create an option (same function update_option()) with the plugin settings.
		//
		// VERY IMPORTANT!
		// If the two keys are the same, the settings will overwrite the version, and vice-versa!
    "PLUGIN_SETTINGS_KEY": "woocommerce-template-plugin",
    "PLUGIN_FILTER_PREFIX": "wc_template_plugin",
    "PLUGIN_WC_MIN_VERSION": "3.0",
    "PLUGIN_WC_MAX_VERSION": "4.8",
    "PLUGIN_TAGS": ""
  }
}
